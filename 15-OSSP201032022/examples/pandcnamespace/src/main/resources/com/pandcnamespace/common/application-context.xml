<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="engine" class="com.pandcnamespace.beans.Engine"
		c:engineId="10" c:model="Isuzu" />
		
	<bean id="car" class="com.pandcnamespace.beans.Car"
		p:regNo="TS09BA1234" p:model="Swift" p:manufacturer="Maruthi"
		p:color="red" p:engine-ref="engine" />
</beans>

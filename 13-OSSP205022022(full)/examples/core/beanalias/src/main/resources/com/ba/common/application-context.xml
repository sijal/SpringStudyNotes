<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="person" name="james,jem" class="com.ba.beans.Person">
		<property name="personNo" value="983" />
		<property name="personName" value="James" />
		<property name="age" value="29" />
		<property name="gender" value="Male" />
	</bean>

	<alias name="james" alias="jimmy" />
	<alias name="jem" alias="jim,carry" />
	<alias name="person" alias="j carry" />
</beans>
